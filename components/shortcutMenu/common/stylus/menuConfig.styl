
width = 50px
height = 50px
corner-margin-left = 40px
corner-margin-bottom = 40px
animationDuriation = .5s
.menu-center-wrapper
  position absolute
  z-index 1000
  top 50%
  right 50%
  width width
  height height
  transform 50% 50%

.menu-left-corner-wrapper
  position fixed
  z-index 1000
  bottom corner-margin-bottom
  left corner-margin-left
  width width
  height height
.menu-right-corner-wrapper
  position fixed
  z-index 1000
  bottom corner-margin-bottom
  right corner-margin-left
  width width
  height height
